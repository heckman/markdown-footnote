{
  "name": "vscode-markdown-footnotes-plus",
  "displayName": "vscode-markdown-footnotes-plus",
  "description": "Better interaction for markdown footnotes in editor",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.47.0"
  },
  "keywords": [
    "markdown",
    "footnote",
    "preview",
    "notes",
    "note taking"
  ],
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-markdown-footnotes-plus.insertFootnote",
        "title": "Insert a footnote"
      }
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "commitlint": "^9.1.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^3.8.3",
    "vsce": "^1.78.0",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "markdown-it-footnote": "^3.0.2"
  }
}
